<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nan.cebo.competition.persistence.CompetitionMapper">

  <resultMap id="resultMap" type="com.nan.cebo.competition.domain.competition.IndexPic">
    <id column="id" property="id"/>
    <result column="comp_id" property="compId"/>
    <result column="pic_name" property="picName"/>
    <result column="pic_url" property="picURL"/>
  </resultMap>
  <sql id="Base_Column_List">
    id, comp_id,pic_name,pic_url
  </sql>

  <select id="getIndexSwiperImage" resultMap="resultMap">
    SELECT
    <include refid="Base_Column_List"/>
    FROM index_pic
  </select>
</mapper>